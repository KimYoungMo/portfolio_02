<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org./dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<!-- camel표기법 : member_name -> memberName -->
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
	<!-- 쿼리 수행 결과를 DTO에 자동 맵핑하기 위해 DTO 검색 -->
	<typeAliases>
	<!-- @Alias("Member") <- class 위에 어노테이션 추가 -->
		<package name="com.kym.pf.dto" />
	</typeAliases>
	
</configuration>